{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Basic React Boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-server && npm run build-client",
    "build-server": "babel src/server --out-dir ./dist",
    "build-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/index.js"
},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NewSky54/React-Boilerplate.git"
  },
  "keywords": [
    "Node",
    "React"
  ],
  "author": "Skyler Escobedo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NewSky54/React-Boilerplate/issues"
  },
  "homepage": "https://github.com/NewSky54/React-Boilerplate#readme",
  "dependencies": {
    "express": "^4.16.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1"
  }
}
